{
    "name": "siniliote-twig/date-extra",
    "type": "library",
    "description": "A Twig extension to transform time diff in human readable",
    "keywords": ["twig", "date", "human-readable"],
    "homepage": "https://twig.symfony.com",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Férey Stéphane",
            "email": "stephane.ferey@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "twig/twig": "^2.7|^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4.9|^5.0.9|^6.0",
        "symfony/translation-contracts": "^1.1|^2|^3"
    },
    "suggest": {
        "symfony/translation": "Allow the time_diff output to be translated"
    },
    "autoload": {
        "psr-4" : { "Twig\\Extra\\Date\\" : "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
